<% if notice != nil %>
  <p class="alert alert-success" id="notice"><%= notice  %></p>
<% end %>


<p>
  <strong>Task:</strong>
  <%= @task.task %>
  <%= @task.due_date %>
  <%= @task.complete %>
</p>

<%= link_to 'Edit', edit_task_path(@task), class: "btn btn-primary" %> |
<%= link_to 'Back', tasks_path, class: "btn btn-primary" %>
